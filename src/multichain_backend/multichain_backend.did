
type PublicKeyReply = record { public_key_hex : text; eth_address: text};
type SignOutput = record { tx_sign : text;};
type Result = variant { Ok : PublicKeyReply; Err : text };
type ResultSignOut = variant { Ok : SignOutput; Err:text };
service : {
    "greet": (text) -> (text) query;
    public_key : () -> (Result);
    sign: () -> (ResultSignOut);
}
